{
  "name": "progtutor",
  "displayName": "ProgTutor",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "interfaz",
          "title": "ProgTutor",
          "icon": "media/robot.png"
        }
      ]
    },
    "views": {
      "interfaz": [
        {
          "type": "webview",
          "id": "progtutor.principal",
          "name": "Principal"
        },
        {
          "type": "webview",
          "id": "progtutor.bibliog",
          "name": "Bibliograf√≠a"
        },
        {
          "type": "webview",
          "id": "progtutor.profesor",
          "name": "Profesor"
        }
      ]
    },
    "commands": [
      {
        "command": "progtutor.ejecutarArchivo",
        "category": "ProgTutor: ",
        "title": "REVISAR"
      },
      {
        "command": "progtutor.abrirWeb",
        "category": "ProgTutor: ",
        "title": "Bibliografia"
      },
      {
        "command": "progtutor.comUnity",
        "category": "ProgTutor: ",
        "title": "Unity"
      },
      {
				"command": "progtutor.libPista1",
				"category": "Principal",
				"title": "Pista 1"
			},
      {
				"command": "progtutor.libPista2",
				"category": "Principal",
				"title": "Pista 2"
			},
      {
				"command": "progtutor.bloqPista1",
				"category": "Principal",
				"title": "Bloqueo Pista 1"
			},
      {
				"command": "progtutor.bloqPista2",
				"category": "Principal",
				"title": "Bloqueo Pista 2"
			},
      {
				"command": "progtutor.execPista1",
				"category": "Principal",
				"title": "Ejecutar Pista 1"
			},
      {
				"command": "progtutor.execPista2",
				"category": "Principal",
				"title": "Ejecutar Pista 2"
			},
      {
				"command": "progtutor.Evaluar",
				"category": "Principal",
				"title": "Mandar a evaluar"
			},
      {
				"command": "progtutor.libEvaluar",
				"category": "Principal",
				"title": "Libero Evaluar"
			},
      {
				"command": "progtutor.bloqEvaluar",
				"category": "Principal",
				"title": "Bloque Evaluar"
			},
      {
				"command": "progtutor.respDuda",
				"category": "Profesor",
				"title": "Duda Resuelta"
			},
      {
        "command": "progtutor.refrecar",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "progtutor.refrecar",
          "when": "view == progtutor.principal",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.4",
    "axios": "^1.4.0",
    "esm": "^3.2.25",
    "fs": "^0.0.1-security",
    "google-translate-api": "^2.3.0",
    "moment": "^2.29.4",
    "mongodb": "^5.6.0",
    "node-fetch": "^3.3.1",
    "puppeteer": "^20.7.3"
  }
}
