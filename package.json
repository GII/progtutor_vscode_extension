{
  "name": "progtutor",
  "displayName": "ProgTutor",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-explorer",
          "title": "ProgTutor",
          "icon": "media/robot.png",
          "command": "progtutor.comUnity"
        }
      ]
    },
    "views": {
      "package-explorer": [
        {
					"type": "webview",
					"id": "progtutor.webview",
					"name": "ProgTutor view"
				}
      ] 
    },
    "commands": [
      {
        "command": "progtutor.ejecutarArchivo",
        "category": "ProgTutor: ",
        "title": "REVISAR"
      },
      {
        "command": "progtutor.abrirWeb",
        "category": "ProgTutor: ",
        "title": "Bibliografia"
      },
      {
        "command": "progtutor.comUnity",
        "category": "ProgTutor: ",
        "title": "Unity"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.4",
    "axios": "^1.4.0",
    "esm": "^3.2.25",
    "google-translate-api": "^2.3.0",
    "moment": "^2.29.4",
    "mongodb": "^5.6.0",
    "node-fetch": "^3.3.1",
    "puppeteer": "^20.7.3"
  }
}
